    "concurrently": "^5.3.0"
  },
  "scripts": {
    "dev": "concurrently \"yarn --cwd client dev\" \"yarn --cwd server dev\"",
    "prod": "concurrently \"yarn --cwd client start\" \"yarn --cwd server prod\"",
